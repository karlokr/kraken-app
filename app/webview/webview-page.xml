<Page xmlns="http://www.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" loaded="pageLoaded" unloaded="pageUnloaded">
    <ActionBar class="action-bar" flat="true" title="{{ url_title }}">
        <NavigationButton text="" icon="~/images/next.png" tap="onNavBtnTap"/>
    </ActionBar>

    <GridLayout rows="*">
        <WebView row="0" loaded="onWebViewLoaded" id="myWebView" src="{{ webViewSrc }}" />
    </GridLayout>

</Page>